{
  "name": "@nativescript/angular",
  "version": "18.1.1",
  "homepage": "https://nativescript.org/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NativeScript/angular.git"
  },
  "bugs": {
    "url": "https://github.com/NativeScript/angular/issues"
  },
  "author": "NativeScript TSC",
  "license": "Apache-2.0",
  "keywords": [
    "NativeScript",
    "Angular"
  ],
  "dependencies": {
    "@nativescript/zone-js": "^3.0.4",
    "tslib": "^2.3.0"
  },
  "sideEffects": [
    "**/nativescript-angular-polyfills.mjs",
    "./polyfills/**"
  ],
  "module": "fesm2022/nativescript-angular.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2022": "./esm2022/nativescript-angular.mjs",
      "esm": "./esm2022/nativescript-angular.mjs",
      "default": "./fesm2022/nativescript-angular.mjs"
    },
    "./polyfills": {
      "types": "./polyfills/index.d.ts",
      "esm2022": "./esm2022/polyfills/nativescript-angular-polyfills.mjs",
      "esm": "./esm2022/polyfills/nativescript-angular-polyfills.mjs",
      "default": "./fesm2022/nativescript-angular-polyfills.mjs"
    },
    "./testing": {
      "types": "./testing/index.d.ts",
      "esm2022": "./esm2022/testing/nativescript-angular-testing.mjs",
      "esm": "./esm2022/testing/nativescript-angular-testing.mjs",
      "default": "./fesm2022/nativescript-angular-testing.mjs"
    }
  }
}